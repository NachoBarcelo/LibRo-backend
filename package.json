{
  "name": "libro-backend",
  "version": "1.0.0",
  "description": "Backend API for LibRo personal library app",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:backfill:external-id": "tsx scripts/backfill-book-external-ids.ts",
    "db:backfill:external-id:apply": "tsx scripts/backfill-book-external-ids.ts --apply",
    "postinstall": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NachoBarcelo/LibRo-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/NachoBarcelo/LibRo-backend/issues"
  },
  "homepage": "https://github.com/NachoBarcelo/LibRo-backend#readme",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "prisma": "^6.16.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
